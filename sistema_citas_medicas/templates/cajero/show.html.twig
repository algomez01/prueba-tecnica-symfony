{% extends 'base.html.twig' %}

{% block title %}Factura{% endblock %}

{% block body %}
    <h1>Factura</h1>

    <table class="table">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ factura.id }}</td>
            </tr>
            <tr>
                <th>Fecha_creacion</th>
                <td>{{ factura.fechaCreacion ? factura.fechaCreacion|date('Y-m-d H:i:s') : '' }}</td>
            </tr>
            <tr>
                <th>Total</th>
                <td>{{ factura.total }}</td>
            </tr>
            <tr>
                <th>CitaId</th>
                <td>{{ factura.citaId }}</td>
            </tr>
            <tr>
                <th>CadejoId</th>
                <td>{{ factura.cadejoId }}</td>
            </tr>
        </tbody>
    </table>

    <a href="{{ path('app_cajero_index') }}">back to list</a>

    <a href="{{ path('app_cajero_edit', {'id': factura.id}) }}">edit</a>

    {{ include('cajero/_delete_form.html.twig') }}
{% endblock %}
